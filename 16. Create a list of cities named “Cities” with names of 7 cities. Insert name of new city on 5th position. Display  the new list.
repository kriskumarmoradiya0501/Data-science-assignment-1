city = ["surat","bhavanagar","mumbai","delhi","panjab","gudgaun"]
for cityies in city:
    print(cityies)
print("Here we are adding Hariyana on 5th position")
city.insert(4, "Haryana")
for cityies in city:
    print(cityies)
