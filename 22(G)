Cities = ("Surat", "Ahmedabad", "Mumbai", "Gurgaon", "Chandigarh", "Lokhandvala", "Lonavala")
for item in Cities:
    print(item)
States = ("Gujarat", "Punjab", "Uttar Pradesh", "Uttarakhand", "Maharashtra", "Tamil Nadu", "Jammu & Kashmir", "Rajasthan")
result = Cities + States
print("Merged tuple of cities and states:")
for item in result:
    print(item)
