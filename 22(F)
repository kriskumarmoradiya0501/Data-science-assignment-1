Cities = ("Surat", "Ahmedabad", "Mumbai", "Gurgaon", "Chandigarh", "Lokhandvala", "Lonavala")
for city in Cities:
    print(city)

Cities = Cities + ("Jaipur",)
print("Tuple of cities after inserting 'Jaipur' at last position:")
for city in Cities:
    print(city)
