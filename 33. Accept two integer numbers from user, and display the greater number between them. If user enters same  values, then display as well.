num1 = int(input("Enter Number 1 : "))
num2 = int(input("Enter Number 2 : "))

if num1>num2:
    print(num1," is big")
elif num2>num1:
    print(num2," is big")
elif num1==num2:
    print(num1," and ",num2," are equal")
