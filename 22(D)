Cities = ("Surat", "Ahmedabad", "Gurgaon", "Chandigarh", "Lokhandvala", "Lonavala")
list1 = list(Cities)
list1[2] = "Jodhapur"
city = tuple(list1)
print("Updated tuple of cities:")
for city1 in city:
    print(city1)
