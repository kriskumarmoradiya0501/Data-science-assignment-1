Cities = ("Surat", "Ahmedabad", "Mumbai", "Gurgaon", "Chandigarh", "Lokhandvala", "Lonavala")
Cities = Cities[:4] + ("Jodhapur",) + Cities[4:]
print("Tuple of cities after inserting a new city at 5th position is ")
for city in Cities:
    print(city)
