city = ["surat","Ahmedabad","Chandigrh","bhavanagar","Hariyana","gudgaun"]
for cityies in city:
    print(cityies)
print("Here we are removing bhavanagar")
city.remove("bhavanagar")
for cityies in city:
    print(cityies)
