Cities = ("Surat", "Ahmedabad", "Mumbai", "Gurgaon", "Chandigarh", "Lokhandvala", "Lonavala")
for city in Cities:
    print(city)
Cities = Cities[:3] + Cities[4:]
print("Tuple of cities after removing the 4th element by value:")
for city in Cities:
    print(city)
